{
    "name": "rdatasci",
    "services": [
        {
            "command": "/usr/local/bin/start_notebook.sh",
            "environment": [],
            "essential_count": 1,
            "image": "zapps/rdatasci:10396",
            "monitor": true,
            "name": "r-notebook",
            "ports": [
                {
                    "name": "Jupyter Notebook interface",
                    "port_number": 8888,
                    "protocol": "tcp",
                    "proxy": true,
                    "url_template": "http://{ip_port}{proxy_path}"
                }
            ],
            "replicas": 1,
            "resources": {
                "cores": {
                    "max": 2,
                    "min": 2
                },
                "memory": {
                    "max": 6442450944,
                    "min": 6442450944
                }
            },
            "startup_order": 0,
            "total_count": 1,
            "volumes": []
        }
    ],
    "size": 512,
    "version": 3,
    "will_end": false
}